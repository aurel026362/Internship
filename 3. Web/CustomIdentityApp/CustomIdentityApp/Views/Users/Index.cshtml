@model IEnumerable<CustomIdentityApp.Models.User>
@{
    ViewData["Title"] = "List of users";
}

<h2>Index</h2>

<a asp-action="Create">Add User</a>

<table class="table">
    <tr>
        <th>Email</th>
        <th>Year</th>
    </tr>
    @foreach(var user in Model)
    {
    <tr>
        <td>@user.Email</td>
        <td>@user.Year</td>
        <td>
            <form asp-action="Delete" asp-route-id="@user.Id" method="post">
                <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-id="@user.Id">
                    Change
                </a>
                <a class="btn btn-sm btn-primary" asp-action="ChangePassword" asp-route-id="@user.Id">Сменить пароль</a>

                <button type="submit" class="btn btn-sm btn-danger">
                    Delete
                </button>
            </form>
        </td>
    </tr>
    }
</table>